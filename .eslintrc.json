{
  "env": {
    "node": true,
    "es2021": true,
    "mocha": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "consistent-return": "off",
    "linebreak-style": "off",
    "camelcase": "off"
  }
}
